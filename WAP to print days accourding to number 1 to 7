//WAP accourding to number check days like 1=monday 2=tuesday
#include<stdio.h>
int main()
{
	int n;
	printf("ENTER THE WEEK DAY NUMBER LIKE 1->MONDAY ,2->TUESDAY\n");
	scanf("%d",&n);
	if(n==1)
	printf("monday");
	else if(n==2)
	printf("tuesday");
	else if(n==3)
	printf("wednesday");
	else if(n==4)
	printf("thursday");
	else if(n==5)
	printf("friday");
	else if(n==6)
	printf("saturday");
	else if(n==7)
	printf("sunday");
	else
	printf("ENTIRE VALUE IS NOT A WEEK NUMBER\n THANKS");
}
*/..............OUTPUT SCREEN.....................
ENTER THE WEEK DAY NUMBER LIKE 1->MONDAY ,2->TUESDAY
7
sunday
--------------------------------
Process exited after 7.586 seconds with return value 0
Press any key to continue . . .*/
