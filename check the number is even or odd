//check the number is even or odd
#include<stdio.h>
int main()
{
	int n;
	printf("Enter an number\n");
	scanf("%d",&n);
	if(n%2==0)
	printf("even");
	else
	printf("odd");
}
/*.................OUTPUT SCREEN...............
Enter an number
4
even
--------------------------------
Process exited after 10.71 seconds with return value 0
Press any key to continue . . .
*/
