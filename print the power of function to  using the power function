// print the power of function to  using the power functoin
#include<stdio.h>
#include<math.h>
int main()
{
	int a,b,p;
	printf("Enter the a number:\n");
	scanf("%d",&a);
	printf("Enter the power of number a:\n");
	scanf("%d",&b);
	p=pow(a,b);
	printf("The exponence of a^b=%d",p);
	return 0;
}
/*.................OUTPUT SCREEN..................
Enter the a number:
2
Enter the power of number a:
3
The exponence of a^b=8
--------------------------------
Process exited after 15.79 seconds with return value 0
Press any key to continue . . .
*/
