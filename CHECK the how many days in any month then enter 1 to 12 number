//Enter 1 to 12 number and find the days in any month like 1 january
#include<stdio.h>
int main()
{
	int m;
	printf("ENTER A MONTH NUMBER B/W 1 TO 12\n");
	scanf("%d",&m);
	if(m>=1&&m<=12)
	{
	if(m==1||m==3||m==5||m==7||m==8||m==10||m==12)
	printf("31 days month");
	else if(m==4||m==6||m==9||m==11)
	printf("30 days month");
	else
	printf("28 or 29 days month ferbuary");
    }
    else
    printf("NO RESULT FOUND");
	
}
/*..................OUTPUT SCREEN.................
ENTER A MONTH NUMBER B/W 1 TO 12
4
30 days month
--------------------------------
Process exited after 11.08 seconds with return value 0
Press any key to continue . . .
*/
