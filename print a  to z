//print a to z
#include<stdio.h>
int main()
{
	char ch=97;
	printf("a to z\n");
	while(ch<=122)
	{
		printf("%c",ch);
		printf("\t");
		ch++;
	}
  return 0;
}
/*...................OUTPUT SCREEN..............
a to z
a       b       c       d       e       f       g       h       i       j       k       l       m       n       o       p       q       r       s       t       u       v       w       x       y       z
--------------------------------
Process exited after 1.383 seconds with return value 0
Press any key to continue . . .*/
