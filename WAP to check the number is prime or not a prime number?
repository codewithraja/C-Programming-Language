//WAP to check the number is prime or not a prime number
#include<stdio.h>
int main()
{
	int n,i,flag=0;
	printf("Enter  a number:\n");
	scanf("%d",&n);
	for(i=2;i<n;i++)
	{
		if(n%i==0)
		{
			flag=1;
		}
    }
    if(flag==1)
    printf("%d is not a prime number",n);
    else
    printf("%d is prime number",n);
    return 0;
}
/*Enter  a number:
2
2 is prime number
*/
